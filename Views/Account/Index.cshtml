
<div class="view-box">
    <h1>
        Welcome, @Model.UserRealName!
    </h1>
    <h2>
        @Model.AgencyName
    </h2>
    <p class="account-nav"><a href="/account/MyAccount">Edit my account</a></p>

    <div class="my-rfps">
        <h2>Current RFPs - 1 open, 1 disabled</h2>
        <p>
            These are your current RFPs.  When an RFP closes, or when you manually disable it, it moves from this table to the inactive RFP table below.
        </p>
        <table class="pure-table pure-table-horizontal">
            <thead>
                <tr>
                    <th>Title</th>
                    <th>Close Date</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr>
                   <td><a target="_blank" href="/account/editRfp?id=1">My Nice RFP</a></td><td>4/2/1978</td><td><a href="">Clone</a> <a href="">Disable</a> <a href="">Close Now</a></td>
                </tr>
            </tbody>
        </table>


        <h2>Historical RFPs - 1 ended</h2>
        <p>
            These are your inactive RFPs.  You can't reopen any of these, but you can clone one and open a new RFP.
        </p>
        <table class="pure-table pure-table-horizontal">
            <thead>
                <tr>
                    <th>Title</th>
                    <th>Close Date</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr>
                   <td><a target="_blank" href="/account/editRfp?id=1">My Nice RFP</a></td><td>4/2/1978</td><td><a href="">Clone</a></td>
                </tr>
            </tbody>
        </table>
    </div>

</div>


<div class="pure-g">
    <div class="pure-u-1 l-box">
        <div class="footnote">

            <div class="pages large-hide">
                <div class="head">Pages</div>
                <ul>
                    @Html.Partial("Pages-menu")
                </ul>
            </div>

            <p class="tip">
                Got questions?  Oh yes you do.
            </p>
        </div>
    </div>
</div>
