<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Find government contracting
                                  opportunities fast!">

    <title>@ViewBag.extraTitle - Procurement Search</title>

    <link rel="alternate" type="application/rss+xml" title="RSS"
          href="http://www.procurementsearch.com/rss">



    <environment names="Development">
        <link rel="stylesheet" href="~/css/pure-min.css">
        <link rel="stylesheet" href="~/css/grids-responsive-min.css">
        <link rel="stylesheet" href="~/css/fonts.css">
        <link rel="stylesheet" href="~/lib/jquery-modal/jquery.modal.css">
        <link rel="stylesheet" href="~/lib/Croppie/croppie.css">
        <link rel="stylesheet" href="~/lib/datetimepicker/jquery.datetimepicker.css">
        <link rel="stylesheet" href="~/lib/blueimp-file-upload/css/jquery.fileupload.css">
        <link rel="stylesheet" href="~/css/styles.css"> <!-- site styles have to go last -->

        <script type="text/javascript" src="~/lib/jquery/dist/jquery.js"></script>
        <script type="text/javascript" src="~/lib/jquery-modal/jquery.modal.js" charset="utf-8"></script>
        <script type="text/javascript" src="~/lib/parsleyjs/dist/parsley.js"></script>
        <script type="text/javascript" src="~/lib/Croppie/croppie.js"></script>
        <script type="text/javascript" src="~/lib/php-date-formatter/js/php-date-formatter.js"></script>
        <script type="text/javascript" src="~/lib/datetimepicker/jquery.datetimepicker.js"></script>
        <script type="text/javascript" src="~/lib/blueimp-file-upload/js/vendor/jquery.ui.widget.js"></script>
        <script type="text/javascript" src="~/lib/blueimp-file-upload/js/jquery.iframe-transport.js"></script>
        <script type="text/javascript" src="~/lib/blueimp-file-upload/js/jquery.fileupload.js"></script>
    </environment>

    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
        <script type="text/javascript" src="~/js/site.min.js"></script>
    </environment>

    <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-55163346-3', 'auto');
    ga('send', 'pageview');

    </script>
</head>
<body>


    <div class="wrapper">
        <!-- for sticky footer -->

        <div class="header small-hide">
            <div class="pure-menu pure-menu-horizontal">
                <a class="pure-menu-heading rss" href="/rss"><img src="/images/rss-icon.png"></a>
                <ul class="pure-menu-list">
                    @Html.Partial("Pages-nav")
                </ul>
            </div>
        </div>
	<div style="clear: both;"></div>

        @RenderBody()

        <div class="push"></div>
    </div>


    <div class="footer pure-g">
        @Html.Partial("Pages-footer")
    </div>





</body>
</html>
